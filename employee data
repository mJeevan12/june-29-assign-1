emp={}
n=int(input("enter no of employees:")
for i in range(1,n+1):
     l=[]
     name=input("enter a employee name:")
     l.append(name)
     designation=input("enter a employee work:")
     l.append(designation)
     phno=int(input('enter employee phno:'))
     l.append(phno)
    emp[i]=1
print(emp)
 
